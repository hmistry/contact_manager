%h1 Editing phone_number

= render 'form'

= link_to 'Show', @phone_number
\|
- if @phone_number.contact_type == "Person"
  = link_to 'Back', phone_numbers_path
- else
  = link_to 'Back', companies_path
\|
- if @phone_number.contact_type == "Person"
  = link_to 'Back to Person', person_path(@phone_number.contact_id)
- else
  = link_to 'Back to Company', company_path(@phone_number.contact_id)
